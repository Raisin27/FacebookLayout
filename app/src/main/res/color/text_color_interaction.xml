<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Trạng thái khi được nhấn -->
    <item android:state_pressed="true" android:color="@color/white" />
    <!-- Trạng thái khi da được nhấn -->
    <item android:state_activated="true" android:color="@color/textBlue" />
    <!-- Trạng thái mặc định -->
    <item android:color="@color/textWhite" />
</selector>